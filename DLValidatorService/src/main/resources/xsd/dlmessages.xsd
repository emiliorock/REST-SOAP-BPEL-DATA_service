<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://soacourse.unsw.edu.au/dldefinitions"
	xmlns:tns="http://soacourse.unsw.edu.au/dldefinitions"
	elementFormDefault="qualified">

	<xsd:complexType name="DLInputType">
		<xsd:sequence>
			<xsd:element name="fullName" type="xsd:string" />
			<xsd:element name="dl" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ValidationType">
		<xsd:sequence>
			<xsd:element name="accept" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="DLInfoMessage" type="tns:DLInputType" />
	<xsd:element name="ValidationMessage" type="tns:ValidationType" />

</xsd:schema>